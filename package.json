{
  "name": "main",
  "packageManager": "yarn@4.4.0",
  "scripts": {
    "lint:eslint": "eslint --fix",
    "lint:prettier": "prettier --write \"**/*.{ts,tsx,mjs,js,css}\"",
    "lint:stylelint": "stylelint \"src/**/*.css\"",
    "lint:tsc": "tsc --noEmit",
    "lint": "concurrently --names \"prettier,eslint,stylelint,tsc\" --prefix-colors gray --pad-prefix --group \"yarn lint:prettier\" \"yarn lint:eslint\" \"yarn lint:stylelint\" \"yarn lint:tsc\"",
    "dev": "gatsby develop",
    "build": "gatsby build --prefix-paths"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@eslint/compat": "^1.2.4",
    "@eslint/js": "^9.17.0",
    "@types/babel__core": "^7.20.5",
    "@types/eslint": "^9.6.1",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^22.10.2",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@types/react-helmet": "^6.1.11",
    "@typescript-eslint/parser": "^8.18.1",
    "concurrently": "^9.1.0",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.7.0",
    "eslint-plugin-import-x": "^4.6.1",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.1.0",
    "gatsby-cli": "^5.14.0",
    "globals": "^15.14.0",
    "prettier": "3.4.2",
    "stylelint": "^16.12.0",
    "stylelint-config-standard": "^36.0.1",
    "stylelint-selector-bem-pattern": "^4.0.1",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.18.1"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "gatsby": "^5.14.1",
    "gatsby-omni-font-loader": "^2.0.2",
    "gatsby-plugin-image": "^3.14.0",
    "gatsby-plugin-robots-txt": "^1.8.0",
    "gatsby-plugin-sharp": "^5.14.0",
    "gatsby-source-filesystem": "^5.14.0",
    "gatsby-transformer-remark": "^6.14.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet": "^6.1.0"
  }
}
